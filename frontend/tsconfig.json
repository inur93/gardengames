{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "compilerOptions": {
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2020"
    ],
    "types": ["node"],
    "isolatedModules": false,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
"exclude": ["node_modules"],
  "references": [
    {
      "path": "./tsconfig.vite-config.json"
    }
  ]
}
